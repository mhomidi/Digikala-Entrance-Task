{% extends 'base.html.twig' %}

{% block title %}Information{% endblock %}

{% block body %}

    <div class="container_fluid text-center" style="margin-top: 100px;">
        <div class="row">
            <div class ="col-4 offset-4">
                <table class="table  table-dark table-striped">
                    <thead>
                    <tr>
                        <th>Number</th>
                        <th>Amount</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr>
                        <td>Number</td>
                        <td id='name'>{{ info.number }}</td>
                    </tr>
                    <tr>
                        <td>All Sent</td>
                        <td id='name'>{{ info.allSent }}</td>
                    </tr>
                    <tr>
                        <td>First Api Success</td>
                        <td id='name'>{{ info.firstAPISent }}</td>
                    </tr>
                    <tr>
                        <td>First Api Fail</td>
                        <td id='name'>{{ info.firstAPIFail }}</td>
                    </tr>
                    <tr>
                        <td>Second Api Success</td>
                        <td id='name'>{{ info.secondAPISent }}</td>
                    </tr>
                    <tr>
                        <td>Second Api Fail</td>
                        <td id='name'>{{ info.secondAPIFail }}</td>
                    </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
{% endblock %}
